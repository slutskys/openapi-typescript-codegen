{{#if description}}
/**
 * {{{escapeComment description}}}
 */
{{/if}}
export enum {{{name}}} {
	{{#each enum}}
	{{#if description}}
	/**
	 * {{{escapeComment description}}}
	 */
	{{/if}}
	{{#containsSpaces name}}
	'{{{name}}}' = {{{value}}},
	{{else}}
	{{{name}}} = {{{value}}},
	{{/containsSpaces}}
	{{/each}}
}
